{
    "name": "nscps/php-functions",
    "description": "Functions that do not deserve a class or component. Not yet.",
    "type": "library",
    "keywords": ["functions", "php", "utilities", "helpers"],
    "homepage": "https://github.com/nscps/php-functions",
    "license": "MIT",
    "authors": [
        {
            "name": "NSCPS Solutions",
            "email": "nscpssolutions@gmail.com"
        },
        {
            "name": "Nilo Soares",
            "email": "nilosoares@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "webmozart/assert": "^1.10",
        "symfony/finder": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.20"
    },
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nscps\\Functions\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "tests": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html=coverage"
        ]
    }
}
